<p style="color: green"><%= notice %></p>

<div class="page-header">
  <h1>All Todos</h1>
</div>

<% @items.each do |item| %>
  <h3>
    <%= item.item %>
    <%= link_to "Edit", edit_item_path(item) %>
    <%= button_to "Destroy", item, method: :delete %>
  </h3>
<% end %>

<%= link_to "New item", new_item_path %>
